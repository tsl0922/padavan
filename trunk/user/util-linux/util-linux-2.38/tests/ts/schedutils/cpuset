TS_TOPDIR="${0%/*}/../.."
TS_DESC="cpuset"

. $TS_TOPDIR/functions.sh
ts_init "$*"

ts_check_test_command "$TS_HELPER_CPUSET"

MASKS=" 0x00000001 \
	0x00000002 \
	0x00000003 \
	0x00000004 \
	0x00000005 \
	0x00000006 \
	0x00000007 \
	0x00000008 \
	0x00000009 \
	0x00005555 \
	0x00007777"

RANGES="0 \
	1 \
	0,1 \
	2 \
	0,2 \
	1,2 \
	0-2 \
	3 \
	0,3 \
	0,2,4,6,8,10,12,14 \
	0-2,4-6,8-10,12-14"

ts_log "masks:"
for i in $MASKS; do
	$TS_HELPER_CPUSET --mask $i >> $TS_OUTPUT
done

ts_log "strings:"
for i in $RANGES; do
	$TS_HELPER_CPUSET --range $i >> $TS_OUTPUT
done

ts_finalize
