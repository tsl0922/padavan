TS_TOPDIR="${0%/*}/../.."
TS_DESC="islocal"

. $TS_TOPDIR/functions.sh
ts_init "$*"

ts_check_test_command "$TS_HELPER_ISLOCAL"

$TS_HELPER_ISLOCAL "$TS_SELF/islocal.data" root nobody "" youngman youngman2 \
    abcdefghx nobo long rot al malformed \
    nonl znobody >> $TS_OUTPUT
ts_finalize
