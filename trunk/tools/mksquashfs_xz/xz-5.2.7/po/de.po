#: src/xz/file_io.c:1038
#, c-format
msgid "%s: Closing the file failed: %s"
msgstr "%s: Fehler beim Schließen der Datei: %s"

#: src/xz/file_io.c:1074 src/xz/file_io.c:1317
#, c-format
