msgstr "  Broj datoteka:            %s\n"

#: src/xz/list.c:1146
msgid "--list works only on .xz files (--format=xz or --format=auto)"
msgstr "--list radi samo sa .xz datoteke (--format=xz ili --format=auto)"

#: src/xz/list.c:1152
