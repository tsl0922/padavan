msgid "Strms  Blocks   Compressed Uncompressed  Ratio  Check   Filename"
msgstr "Tok'i  Blok'i   Komprim'no Dekomprim'no  Omjer  Kontrol Datoteka"

#: src/xz/list.c:717
#, c-format
msgid "  Streams:            %s\n"
msgstr "  Tok(a/ova):               %s\n"

#: src/xz/list.c:719
#, c-format
