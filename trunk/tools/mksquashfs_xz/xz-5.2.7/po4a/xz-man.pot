#: ../src/scripts/xzmore.1:10
msgid "xzmore, lzmore - view xz or lzma compressed (text) files"
msgstr ""

#. type: Plain text
#: ../src/scripts/xzmore.1:13
msgid "B<xzmore> [I<file...>]"
msgstr ""

#. type: Plain text
#: ../src/scripts/xzmore.1:16
msgid "B<lzmore> [I<file...>]"
